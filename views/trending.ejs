<%- include("menu") -%>
    
    <div class="loader">
        <img src="/assets/images/twitter-rise.gif" style="border-radius: 50%;" alt="Loading" />
    </div>
    <%- include("add_comment") -%>
        <!-- main panel  -->
        <div class="col-lg-6 col-md-6 col-sm-8 col-12 px-0 border">
            <div class="inner-main home">
                
                <div class="blur-ui p-3 pt-1">
                    <div class="row flex-row justify-content-between align-items-center my-2 position-relative">
                        <div class="col-1">
                            <a href="javascript: history.go(-1)" class="fa-solid fa-arrow-left-long text-dark d-inline-block me-3 "></a>
                        </div>
                        <div class="col-11">
                            <div class="search-box position-relative">
                                <input type="search" class="rounded-pill w-100" placeholder="Search Anything..."  id="searchTweetHash" oninput="searchSuggestionsTrend(event)" onclick="searchSuggestionsTrend(event)" onblur="hideSuggestion()">
                                <i class="fa-solid fa-magnifying-glass search-icon"></i>

                                <div class="position-absolute d-none" id="searchSuggestion">

                                </div>
                            </div>
                        </div>
                        <!-- <div class="col-1">
                            •••
                        </div> -->
                       
                    </div>
                </div>
                <div class="all-data">
                    <div class="tab-content">
                        <div id="for-you" class="tab-pane active">
                            <div class="tweet-container all-center flex-column" id="forYouPart">

                            </div>
                        </div>
                        <div id="following-home" class="tab-pane">
                            <div class="tweet-container all-center flex-column " id="followingShowData">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script>
            let sel = `<%= sel %>`;
        </script>
        <script src="/javascripts/dashboard.js"></script>

        <%- include("search") -%>